  "version": "0.2.0",
  "configurations": [
    {
      "name": "LD47",
      "request": "launch",
      "program": "${workspaceFolder}/bin/LD47/LD47",
      "linux": {
        "type": "cppdbg",
        "MIMode": "gdb",
        "preLaunchTask": "make",
        "miDebuggerPath": "/usr/bin/gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
      },
      "type": "cppvsdbg",
      "windows": {
        "preLaunchTask": "msbuild",
      },
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
    }
  ]
}
